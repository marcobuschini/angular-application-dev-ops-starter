{
  "name": "frontend",
  "version": "0.0.0",
  "main": "main.js",
  "build": {
    "appId": "marcobuschini.test.app",
    "mac": {
      "category": "your.app.category.type"
    }
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:electron": "ng build --base-href ./ && cp -R config dist/frontend && electron .",
    "start:electron:prod": "ng build --prod --base-href ./ && electron .",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "electron:pack": "ng build --base-href ./ && electron-builder  --dir",
    "electron:dist": "ng build --base-href ./ && electron-builder",
    "test": "npx jest --coverage",
    "test:ci": "npx jest --coverage --coverageReporters lcovonly",
    "lint": "ng lint",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "e2e": "ng e2e",
    "e2e:ci": "npx start-server-and-test start http://localhost:4200 cy:run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.9",
    "@angular/common": "~12.2.9",
    "@angular/compiler": "~12.2.9",
    "@angular/core": "~12.2.9",
    "@angular/forms": "~12.2.9",
    "@angular/platform-browser": "~12.2.9",
    "@angular/platform-browser-dynamic": "~12.2.9",
    "@angular/router": "~12.2.9",
    "rxjs": "~6.5.4",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.9",
    "@angular-eslint/builder": "12.5.0",
    "@angular-eslint/eslint-plugin": "12.5.0",
    "@angular-eslint/eslint-plugin-template": "12.5.0",
    "@angular-eslint/schematics": "12.5.0",
    "@angular-eslint/template-parser": "12.5.0",
    "@angular/cli": "^12.2.9",
    "@angular/compiler-cli": "^12.2.9",
    "@angular/language-service": "^12.2.9",
    "@cypress/webpack-preprocessor": "5.9.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.3",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "commitizen": "^4.2.4",
    "coveralls": "^3.1.1",
    "cypress": "^7.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "electron": "^15.1.1",
    "electron-builder": "^22.11.7",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-prefer-arrow": "latest",
    "eslint-plugin-react": "latest",
    "husky": "^4.2.5",
    "jest": "^27.2.5",
    "jest-html-reporters": "^2.1.6",
    "jest-junit": "^13.0.0",
    "jest-preset-angular": "^10.0.1",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "ts-jest": "^27.0.5",
    "ts-loader": "9.2.6",
    "ts-node": "~10.2.1",
    "typescript": "~4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "pre-commit": "npx pretty-quick --staged && ng lint && npm run test",
      "pre-push": "ng build"
    }
  }
}
