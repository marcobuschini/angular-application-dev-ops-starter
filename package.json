{
  "name": "frontend",
  "version": "0.0.0",
  "main": "main.js",
  "build": {
    "appId": "marcobuschini.test.app",
    "mac": {
      "category": "your.app.category.type"
    }
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:electron": "ng build --base-href ./ && cp -R config dist/frontend && electron .",
    "start:electron:prod": "ng build --prod --base-href ./ && electron .",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "electron:pack": "ng build --base-href ./ && electron-builder  --dir",
    "electron:dist": "ng build --base-href ./ && electron-builder",
    "test": "ng test --coverage",
    "test:ci": "ng test --coverage --coverageReporters lcovonly",
    "lint": "ng lint",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "e2e": "ng e2e",
    "e2e:ci": "npx start-server-and-test start http://localhost:4200 cy:run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.14",
    "@angular/common": "~11.2.14",
    "@angular/compiler": "~11.2.14",
    "@angular/core": "~11.2.14",
    "@angular/forms": "~11.2.14",
    "@angular/platform-browser": "~11.2.14",
    "@angular/platform-browser-dynamic": "~11.2.14",
    "@angular/router": "~11.2.14",
    "rxjs": "~6.5.4",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "^11.2.0",
    "@angular-devkit/build-angular": "^0.1102.14",
    "@angular/cli": "^11.2.14",
    "@angular/compiler-cli": "^11.2.14",
    "@angular/language-service": "^11.2.14",
    "@cypress/webpack-preprocessor": "5.9.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.3",
    "codelyzer": "^6.0.2",
    "commitizen": "^4.2.4",
    "coveralls": "^3.1.1",
    "cypress": "^7.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "electron": "^15.1.1",
    "electron-builder": "^22.11.7",
    "husky": "^4.2.5",
    "jest": "^27.2.5",
    "jest-html-reporters": "^2.1.6",
    "jest-junit": "^13.0.0",
    "jest-preset-angular": "^10.0.1",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "ts-jest": "^27.0.5",
    "ts-loader": "9.2.6",
    "ts-node": "~10.2.1",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.0.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "pre-commit": "npx pretty-quick --staged && ng lint && ng test",
      "pre-push": "ng build"
    }
  }
}
